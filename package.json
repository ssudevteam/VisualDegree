{
  "name": "visualdegree",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run db\" \"npm run test\"",
    "render": "npm install && cd blog && npm install && cd .. && npm run build:website && npm run build:webapp && node main",
    "build:website": "rimraf ./public/website && vite build -c website/vite.config.js && node scripts/website/postbuild",
    "build:webapp": "rimraf ./public/webapp && cross-env VITE_ENV=development vite build -c webapp/vite.config.ts",
    "build:render": "rimraf ./public/webapp && cross-env VITE_ENV=production vite build -c webapp/vite.config.ts",
    "test": "vite serve -c webapp/vite.config.ts",
    "db": "cd database && node server",
    "setup:db": "cd database && npm install && npm run dev",
    "setup:blog": "cd blog && npm install && npm run prod",
    "format": "prettier --check --write \"./**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "debug": "~2.6.9",
    "express-session": "^1.17.3",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "fuze": "^0.0.1",
    "graphql": "^16.6.0",
    "http-proxy-middleware": "^2.0.6",
    "mongoose": "^7.2.2",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oidc": "^0.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-draggable-svg": "^0.0.3",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "reactflow": "^11.7.2",
    "swiper": "^10.1.0",
    "uuid": "^9.0.0",
    "vite": "^4.3.9",
    "vite-plugin-html": "^3.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.2.7",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "react-select": "^5.7.4",
    "rimraf": "^5.0.1",
    "vite-plugin-html-purgecss": "^0.1.1",
    "vite-plugin-static-copy": "^0.16.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
